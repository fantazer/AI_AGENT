						.tariff__list-el-text Обучение
					.tariff__list-el
						.tariff__list-el-icon
							+icon("close","icon icon--false")
						.tariff__list-el-text Консультации
				div(data-modal="order").modal-get.tariff-get Купить

			.tariff__el.tariff__el--lg.wow.fadeInLeft(data-wow-duration=".6s" data-wow-delay="1s")
				.tariff__el-head Professional
				.tariff__el-title
					.tariff__el-title-text 500 000
					.tariff__el-title-sub рублей/год
				.tariff__el-text Режим работы on-site
				.tariff__el-text Неограниченное количество пользователей
				.tariff__list
					.tariff__list-el
						.tariff__list-el-icon
							+icon("check","icon icon--true")
