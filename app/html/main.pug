				div(data-modal="order").modal-get.tariff-get Купить

//contact
.section.section--gray#contanct
	.main-cont
		.section-title остались вопросы?
		.contact.wow.fadeIn(data-wow-duration=".8s" data-wow-delay=".2s")
			.contact-info
				.contact-text Позвоните или напишите нам, чтобы узнать интересующую Вас информацию
				.contact-list
					.contact-list__el
						.contact-list__el-icon
							+icon("adres","icon")
						.contact-list__el-content
							.contact-list__el-title Адрес
							.contact-list__el-text г. Москва, ул. Каланчевская 15, офис 402
					.contact-list__el
						.contact-list__el-icon
							+icon("phone","icon")
						.contact-list__el-content
							.contact-list__el-title Телефон
							a(href="tel:+7 (495) 981-57-10").contact-list__el-text +7 (495) 981-57-10
					.contact-list__el
						.contact-list__el-icon
							+icon("mail","icon")
						.contact-list__el-content
							.contact-list__el-title Эл. адрес
							a(href="mailto:info@itexpert.ru").contact-list__el-text info@itexpert.ru
					.contact-list__el
						.contact-list__el-icon
							+icon("web","icon")
						.contact-list__el-content
							.contact-list__el-title Сайт
							a(href="www.itexpert.ru").contact-list__el-text www.itexpert.ru
			.contact-form
				.contact-text Или просто заполните форму обратной связи, и мы сами свяжемся с Вами
				form.validate-form( action="javascript:void(null);")
					.input-row
						input(type="text" name="name" placeholder="Ваше имя").input
					.input-row
						input(type="text" name="phone" placeholder="Ваш телефон").input
					.input-row
						textarea(name="info", cols="30", rows="5" placeholder="Сообщение").input
					.agree-row
						.check-el
							label.style-label.style-label--checkbox
								input(type="checkbox" name="agree").sytle-input
								.sytle-input-element
									+icon("checked","icon-check")
								.agree-row__text Согласен с
									a(href="#") условиями обработки персональных данных
					input(type="submit" value="Отправить").form-get

.footer

.modal-layer
	.modal-filter
	.modal-wrap
		.modal(data-modal="order")
			.modal-content
				.modal-close
					+icon("close","icon")
				.modal-title Оставьте заявку
				.modal-text Обращаетесь к нам впервые? <br> Оформите обращение с помощью этой формы, мы перезвоним Вам и проконсультируем по любому Вашему вопросу совершенно бесплатно!
				form.validate-form( action="javascript:void(null);")
					.input-row
						input(type="text" name="name" placeholder="Ваше имя").input
					.input-row
						input(type="text" name="phone" placeholder="Ваш телефон").input
					.input-row
						textarea(name="info", cols="30", rows="5" placeholder="Сообщение").input
					.agree-row
						.check-el
							label.style-label.style-label--checkbox
								input(type="checkbox" name="agree").sytle-input
								.sytle-input-element
									+icon("checked","icon-check")
								.agree-row__text Согласен с
									a(href="#") условиями обработки персональных данных
					input(type="submit" value="Отправить заявку").modal-send
		//success message
		.modal(data-modal="truemessage")
			.modal-content.modal-content--true
				+icon("interface","icon")
				.modal-text-true Ваша заявка принята
